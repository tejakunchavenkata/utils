set term png small
set output "~/Simulations.png"
set grid 
set title 'Simulated Results' 
set xlabel 'No of Hits' 
set ylabel 'No of Trials' 
set xrange [0:20000]
set yrange [0:100000]
load "GoldSims/SubPlot"
