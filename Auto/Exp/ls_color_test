#! /bin/csh
foreach n (00 01 04 05 30 31 32 33 34 35 36 37 40 41 42 43 44 45 46 47)
	setenv	LS_COLORS	"$LS_COLORS""*Color_Codes_LS_COLORS=$n"":"
        echo "$LS_COLORS"
	echo $n
	ls Color_Codes_LS_COLORS
end
